{
  "name": "frosted-canvas",
  "version": "2.0.0",
  "description": "Interactive shader gradients for any div element",
  "type": "module",
  "main": "./dist/frosted-canvas.umd.js",
  "module": "./dist/frosted-canvas.es.js",
  "exports": {
    ".": {
      "import": "./dist/frosted-canvas.es.js",
      "require": "./dist/frosted-canvas.umd.js"
    }
  },
  "files": [
    "dist/*.js",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:demo": "vite build --config vite.demo.config.js",
    "preview": "vite preview"
  },
  "keywords": [
    "shader",
    "gradient",
    "three.js",
    "webgl",
    "canvas",
    "background"
  ],
  "author": "SahilK-027",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SahilK-027/frosted-canvas"
  },
  "peerDependencies": {
    "three": "^0.181.0"
  },
  "devDependencies": {
    "three": "^0.181.0",
    "vite": "^6.0.0",
    "vite-plugin-glsl": "^1.3.1"
  },
  "dependencies": {
    "lil-gui": "^0.21.0"
  }
}
